<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.game.dabba.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.game.dabba.tictactoe.HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.game.dabba.tictactoe.gamePlay" />
        <activity android:name="com.game.dabba.tictactoe.statistics" />
        <activity android:name="com.game.dabba.tictactoe.gameSettings"></activity>
    </application>

</manifest>